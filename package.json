{
  "name": "llmops-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "oxlint --fix",
    "format": "oxfmt --write src/",
    "format:check": "oxfmt --check src/",
    "stylelint": "stylelint \"src/**/*.{css,scss}\" --fix",
    "type-check": "tsc --noEmit -p tsconfig.app.json",
    "analyze": "cross-env ANALYZE=true vite build",
    "prepare": "husky",
    "commit": "git-cz"
  },
  "dependencies": {
    "@ant-design/icons": "^6.1.0",
    "antd": "^6.2.2",
    "axios": "^1.13.4",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.19",
    "framer-motion": "^12.29.2",
    "immer": "^11.1.3",
    "lucide-react": "^0.563.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-hook-form": "^7.71.1",
    "react-router-dom": "^7.13.0",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.4.0",
    "@commitlint/config-conventional": "^20.4.0",
    "@types/node": "^25.2.0",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "babel-plugin-react-compiler": "^1.0.0",
    "commitizen": "^4.3.1",
    "cross-env": "^10.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "oxfmt": "^0.27.0",
    "oxlint": "^1.42.0",
    "postcss": "^8.5.6",
    "postcss-preset-env": "^11.1.2",
    "rollup-plugin-visualizer": "^6.0.5",
    "sass": "^1.97.3",
    "stylelint": "^17.1.0",
    "stylelint-config-recess-order": "^7.6.0",
    "stylelint-config-standard-scss": "^17.0.0",
    "stylelint-order": "^7.0.1",
    "typescript": "~5.9.3",
    "vite": "8.0.0-beta.11"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "oxlint --fix",
      "oxfmt --write"
    ],
    "*.{css,scss}": [
      "stylelint --fix",
      "oxfmt --write"
    ]
  },
  "overrides": {
    "vite": "npm:rolldown-vite@8.0.0-beta.7"
  }
}
